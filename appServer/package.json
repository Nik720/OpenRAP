{
  "name": "OpenRAP",
  "version": "1.0.0",
  "productName": "DIKSHA",
  "description": "OpenRAP Software",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/pm2 start index.js --name 'app'",
    "stop": "./node_modules/.bin/pm2 stop 'app'",
    "electron-start": "electron main",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -m --x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "local.sunbird.desktop",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": false
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@project-sunbird/ext-framework-server": "0.1.18",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "connect-multiparty": "^2.1.0",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "form-data": "^2.3.2",
    "getmac": "^1.4.3",
    "glob": "^7.1.2",
    "is-online": "^8.1.0",
    "jquery": "^3.4.0",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.2",
    "node-cron": "^1.2.1",
    "node-zip": "^1.1.1",
    "openrap-sunbirded-plugin": "0.0.6",
    "q": "^1.5.1",
    "random-mac": "0.0.4",
    "request": "^2.87.0",
    "uniqid": "^5.0.3",
    "unzip2": "^0.2.5",
    "winston": "^2.4.2",
    "xlsx": "^0.14.0"
  },
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "pm2": "^3.4.1"
  }
}